<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hyupb.letswalk">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".ManboService"
            android:enabled="true">
            <intent-filter>
                <action android:name="app.manbo.Service" />
            </intent-filter>
        </service>

        <!--<activity android:name=".Today" />-->
        <!--<activity android:name=".Report" />-->
        <!--<activity android:name=".More" />-->
        <!--<activity android:name=".WalkingTrails"></activity>-->
    </application>

</manifest>